{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AutoFacModuleConfiguration": {
    "Repositories": {
      "Suffix": "Repository"
    },
    "Services": {
      "Suffix": "Service"
    },
    "Validator": {
      "Suffix": "Validator"
    }
  },

  "JwtConfiguration": {
    "Token": {
      "ValidAudience": "",
      "ValidIssuer": "Repository",
      "IssuerSigningKey": "Repository"
    }
  },

  "SwaggerConfiguration": {
    "Version": "v1",
    "Title": "Product API",
    "LicenseName": "MIT",
    "LicenseUri": "https://opensource.org/licenses/MIT",
    "SwaggerEndpoint": {
      "Url": "/swagger/v1/swagger.json",
      "Name": "Product API V1"
    }
  },

  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ProductConnection": "Server=.\\SQLExpress;Database=ProductDatabase;Trusted_Connection=True;MultipleActiveResultSets=True;"
  }
}
